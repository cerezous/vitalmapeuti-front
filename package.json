  "scripts": {
    "start": "cd frontend && HOST=0.0.0.0 react-scripts start",
    "build": "cd frontend && DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "postinstall": "cd frontend && npm install && echo 'Dependencies installed successfully'",
    "test": "cd frontend && react-scripts test",
    "eject": "cd frontend && react-scripts eject",
    "cap:sync": "cd frontend && npx cap sync",
    "cap:copy": "cd frontend && npx cap copy",
    "cap:update": "cd frontend && npx cap update",
    "ios:open": "cd frontend && npx cap open ios",
    "ios:sync": "cd frontend && npm run build && npx cap sync ios",
    "ios:build": "cd frontend && npm run build && npx cap sync ios && npx cap open ios"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
